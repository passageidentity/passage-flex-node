{
    "name": "@passageidentity/passage-flex-node",
    "version": "0.1.0",
    "author": "Passage Identity",
    "description": "Node.js client SDK for Passage Passkey Flex",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/passageidentity/passage-flex-node.git"
    },
    "license": "MIT",
    "keywords": [
        "passage",
        "identity",
        "passageidentity",
        "authentication",
        "login"
    ],
    "main": "lib/cjs/index.js",
    "module": "lib/esm/index.js",
    "types": "lib/esm/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "require": "./lib/cjs/index.cjs",
            "import": "./lib/esm/index.js",
            "types": "./lib/esm/index.d.ts"
        }
    },
    "files": [
        "lib/",
        "CHANGELOG.md"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "lint": "eslint \"{src,test}/**/*.ts\" --fix",
        "lint:ci": "eslint \"{src,test}/**/*.ts\"",
        "format": "pretty-quick --staged",
        "prepare": "husky",
        "dev": "ts-node-dev --respawn testServer.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "tsc": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
        "change": "beachball change",
        "publish": "beachball publish -y",
        "beachball-bump": "beachball bump",
        "beachball-check": "beachball check"
    },
    "dependencies": {
        "jose": "^5.2.3",
        "node-fetch": "^2.7.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.30",
        "@types/node-fetch": "^2.6.11",
        "@types/pem": "^1.14.4",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^7.4.0",
        "@typescript-eslint/parser": "^7.4.0",
        "beachball": "^2.41.0",
        "dotenv": "^16.4.5",
        "eslint": "^8.57.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "express": "^4.19.2",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.2",
        "prettier": "^3.2.5",
        "pretty-quick": "^4.0.0",
        "supertest": "^6.3.4",
        "ts-jest": "^29.1.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.4.3"
    }
}
